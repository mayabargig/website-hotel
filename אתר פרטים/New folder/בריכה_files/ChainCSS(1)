@media (max-width: 991px) {
  .results-module {
    display: flex;
    flex-direction: column-reverse;
  }
  .cal0 {
    margin-top: 30% !important;
  }
  .room-paxs-content {
    padding-bottom: 40%;
    width: 100%;
  }
  .daterangepicker .calendars-holder .calender-content .calendar .calendar-table .div-table .div-body {
    padding: 0;
  }
  .daterangepicker .calendars-holder .calender-content .calendar .calendar-table .div-table .div-body .div-row {
    justify-content: center;
  }
  .engine-open {
    overflow: hidden;
  }
  .new-promocode-box {
    text-align: left;
    margin-bottom: 15px;
  }
  #booking-engine-holder .engine-holder #promoNew .promo-holder .promo-input-holder #promo {
    text-align: start;
    padding: 0 50px;
  }
  #booking-engine-beproc #booking-engine-holder.usePromoCodeNew {
    min-height: 340px;
  }
  #booking-engine-holder .engine-holder .bottom-engine-holder .new-promocode-box #promoNew .BtnArea {
